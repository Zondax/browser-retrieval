{
  "name": "Filecoin Retrieval",
  "version": "0.0.1",
  "description": "A p2p network of Filecoin content retrievers",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_page": "options.html",
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/filecoin-16x16.png",
      "32": "images/filecoin-32x32.png",
      "48": "images/filecoin-48x48.png",
      "128": "images/filecoin-128x128.png"
    }
  },
  "icons": {
    "16": "images/filecoin-16x16.png",
    "32": "images/filecoin-32x32.png",
    "48": "images/filecoin-48x48.png",
    "128": "images/filecoin-128x128.png"
  },
  "manifest_version": 2
}
